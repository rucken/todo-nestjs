{
  "name": "rucken-todo",
  "version": "2.1.0",
  "engines": {
    "node": ">=10",
    "npm": ">=6.4.0"
  },
  "private": true,
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "description": "Core with UI for web and native todo application maked on Angular7+",
  "bugs": {
    "url": "https://github.com/rucken/todo/issues"
  },
  "homepage": "https://github.com/rucken/todo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucken/todo.git"
  },
  "keywords": [
    "ngx",
    "angular7",
    "ui",
    "web application",
    "seed",
    "i18n",
    "bootstrap",
    "ssr",
    "todo"
  ],
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-core": "ng build @rucken/todo-core --prod",
    "lib:build-web": "ng build @rucken/todo-web --prod",
    "lib:build": "run-s prepare:prod lib:build-core lib:build-web",
    "app:prebuild": "ng build todo --prod -c=production",
    "app:prebuild:dev": "ng build todo --prod -c=dev",
    "app:build": "run-s prepare:mono app:prebuild",
    "app:build:dev": "run-s prepare:mono app:prebuild:dev",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve -c=production",
    "serve:dev": "ng serve -c=dev",
    "start": "run-s ssr:start",
    "start:prod": "run-s prepare:mono serve",
    "start:dev": "run-s prepare:mono serve:dev",
    "build": "run-s clean format config:mono app:build ssr:build",
    "build:dev": "run-s clean format config:mono app:build:dev ssr:build:dev",
    "test": "npm run app:build",
    "lint": "ng lint",
    "clean": "./node_modules/.bin/del-cli dist",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "e2e": "ng e2e",
    "update": "ng update --all",
    "ssr:prebuild": "ng run todo:server:production",
    "ssr:prebuild:dev": "ng run todo:server:dev",
    "ssr:webpack": "webpack --progress --colors",
    "ssr:start": "node ./server.js",
    "ssr:build": "run-s ssr:prebuild ssr:webpack",
    "ssr:build:dev": "run-s ssr:prebuild:dev ssr:webpack",
    "postinstall": "node ./scripts/patch.js",
    "github:prebuild": "ng build todo --base-href /todo/ --prod",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/todo-client/index.html ./dist/todo-client/404.html",
    "github:build": "run-s lib:build config:prod github:prebuild github:postbuild",
    "github:deploy": "gh-pages -d dist/todo-client"
  },
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/compiler": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/forms": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/platform-server": "^7.0.4",
    "@angular/router": "^7.0.4",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngx-meta/core": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@rucken/core": "^3.4.0",
    "@rucken/web": "^3.4.0",
    "@types/dotenv": "^6.1.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/luxon": "^1.4.1",
    "@types/node": "^10.12.9",
    "@types/object-path": "^0.11.0",
    "core-js": "^2.5.7",
    "cors": "~2.8.5",
    "dotenv": "^6.1.0",
    "jsonwebtoken": "^8.4.0",
    "luxon": "^1.7.1",
    "ngx-bootstrap": "^3.1.1",
    "ngx-cookie-service": "^2.0.0",
    "ngx-dynamic-form-builder": "^0.7.0",
    "ngx-permissions": "^6.0.4",
    "ngx-repository": "^0.6.0",
    "node-fetch": "^2.3.0",
    "object-path": "^0.11.4",
    "rxjs": "^6.3.3",
    "ts-loader": "5.3.3",
    "webpack-cli": "^3.1.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular-devkit/build-ng-packagr": "~0.12.1",
    "@angular/cli": "^7.0.6",
    "@angular/compiler-cli": "^7.0.4",
    "@angular/language-service": "^7.0.4",
    "@rucken/cli": "^3.2.3",
    "@types/body-parser": "^1.17.0",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "bootstrap-sass": "^3.3.7",
    "bootswatch-sass": "^3.2.0",
    "codelyzer": "~4.5.0",
    "conventional-changelog-cli": "^2.0.11",
    "conventional-commits-detector": "^1.0.2",
    "conventional-recommended-bump": "^4.0.4",
    "cookie-parser": "^1.4.3",
    "cp-cli": "^1.1.2",
    "del-cli": "^1.1.0",
    "domino": "^2.1.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lite-server": "^2.4.0",
    "markdown-loader": "^4.0.0",
    "ng-packagr": "^4.4.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.2",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.12.1",
    "typescript": "~3.1.6"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "rxjs",
      "ts-loader",
      "webpack-cli",
      "zone.js"
    ]
  }
}
